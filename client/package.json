{
  "name": "my-github",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --host",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --check \"**/*.{js,ts,vue}\"",
    "lint": "eslint \"**/*.{js,ts,vue}\" --fix",
    "lint:ci": "eslint \"**/*.{js,ts,vue}\"",
    "stylelint": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "codestyle": "run-p prettier lint stylelint"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.32.6",
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.62.0",
    "stylelint": "^15.6.0",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.8.4"
  }
}
